<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GongGeWidget">
        <attr name="column" format="integer"/>
        <attr name="row" format="integer"/>
        <attr name="horizontalSpace" format="dimension"/>
        <attr name="verticalSpace" format="dimension"/>
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="direction" format="enum">
            <enum name="start" value="0"/>
            <enum name="end" value="1"/>
            <enum name="between" value="2"/>
        </attr>
    </declare-styleable>
</resources>